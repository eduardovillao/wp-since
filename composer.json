{
  "name": "eduardovillao/wpsince",
  "description": "Analyze your plugin's compatibility with specific WordPress versions using @since tags, readme.txt, and block API.",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "nikic/php-parser": "^4.15"
  },
  "autoload": {
    "psr-4": {
      "WP_Since\\": "src/"
    }
  },
  "scripts": {
    "generate-since": "php generate-since-json.php",
    "tests:unit": "phpunit tests --testdox --colors",
    "lint": "phpcs",
    "lint:fix": "phpcbf"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.12"
  }
}
